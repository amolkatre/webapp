pipeline {
    agent any
    
    stages {
        stage('Compile stage') {
            steps {
                sh 'mvn clean compile'
            }
        }
        stage('Testing stage') {
            steps {
                sh 'mvn test'
            }
        }
        stage('Install stage') {
            steps {
                sh 'mvn install'
            }
        }
        stage('Echo Branch') {
            steps {
                echo 'echo this is master branch'
            }
        }
    }
}
